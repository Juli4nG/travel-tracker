{
  "name": "travel-tracker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run css:build && bun --hot src/server.ts",
    "start": "bun run css:build && bun src/server.ts",
    "css:build": "tailwindcss -i ./src/styles.css -o ./src/styles.built.css",
    "css:watch": "tailwindcss -i ./src/styles.css -o ./src/styles.built.css --watch",
    "seed": "bun src/seed.ts"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.469.0",
    "date-fns": "^4.1.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49"
  }
}
